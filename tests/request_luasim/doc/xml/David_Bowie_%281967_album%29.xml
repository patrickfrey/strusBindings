<text> <P/>(<C>1981</C><P/>), </text>
<entity lv="2"><text><E>Rock Reflections</E></text>
  </entity>
<text> <P/>(<C>1990</C><P/>), and </text>
<entity lv="2"><pagelink id="The Deram Anthology 1966–1968"><text><E>The Deram Anthology 1966–1968</E></text>
